<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>{{#if title}} {{title}} {{else}} {{titleize page.basename}} {{/if}} | {{ site.title}}</title>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,300,600,700' rel='stylesheet' type='text/css'>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {{#unless cheatsheet}}<link rel="stylesheet" href="assets/css/markdown.css">{{/unless}}
    {{#if cheatsheet}}<link rel="stylesheet" href="assets/css/gist.css">{{/if}}
    <link rel="stylesheet" href="assets/css/assemble.css">
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="assets/js/html5shiv.js"></script>
    <script src="assets/js/respond/respond.min.js"></script>
    <![endif]-->

    <!-- Favicons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="assets/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="assets/ico/favicon.png">
    {{#if site.config.analytic}}{{> analytics }}{{/if}}
    {{#if site.config.schema}}{{> metadata }}{{/if}}
  </head>
  <body style="background: white;" data-spy="scroll" data-target=".bs-sidebar">

    {{!-- NAVIGATION
    =================================== --}}
    {{> navbar }}

    {{!-- MASTHEAD
    =================================== --}}
    <header class="masthead subhead">
      <div class="container">
        <div class="row">
          <div class="col col-lg-12">
            {{> btn-tweet }}
            <h1><span class="text-muted">SDK /&nbsp;</span> {{#if section}}{{ section }} {{else}}{{pages.basename}}{{/if}}</h1>
          </div>
        </div>
      </div>
    </header>
    <div class="container">
      <div class="row">

        {{!-- VERTICAL NAV
        =================================== --}}
        <div class="col col-lg-3">
          <div class="sidebar">

            <ul class="nav nav-pills nav-stacked">
              {{#each pages}}
                <li{{#is ../page.dest this.dest}} class="active"{{/is}}><a href="{{relative ../page.dest this.dest}}" onclick="_gaq.push(['_trackEvent', 'Sidenav', 'clicked', '{{data.section}}/{{data.title}}']);">{{data.section}}</a></li>
              {{/each}}
            </ul>


          </div>
        </div>

        {{!-- MAIN CONTENT
        =================================== --}}
        <div class="col col-lg-9">
          <div class="panel panel-docs">
            <h1>{{page.title}}</h1>
              {{> body }}
            {{#markdown}}
              {{> links-generated.md }}
            {{/markdown}}
            <hr>
            <p><a href="https://github.com/hariadi/viewer/blob/master/src/templates/pages/docs/{{this.basename}}.hbs">See the template for this page → </a></p>
            <div class="alert">

              {{!-- Special styles for GitHub Buttons
              =========================================== --}}
              <style type="text/css" media="screen">
                .alert iframe {
                  float: right;
                  margin: 7px -30px 0 0;
                  vertical-align: text-top;
                }
              </style>

              {{!-- GITHUB BUTTONS (right aligned)
              =========================================== --}}
              {{> btn-github-star }}
              {{> btn-github-fork }}

              <p>Find an error? <a class="alert-link" href="https://github.com/hariadi/viewer/issues?state=open">Let us know →</a></p>
            </div>
            {{> colophon }}
          </div>
        </div>
      </div>
    </div>

    {{!-- SCRIPTS
    =================================== --}}
    {{> javascripts }}
    <script>
      $().showUp('.navbar');
    </script>
  </body>
</html>
