---
title: Tool
layout: android.hbs
desc: Android Repository Viewer
section: Tool
slug: tool
---
{{#if repository.sdk-repository.tool}}
  <h2>tool</h2>
  <dl class="dl-horizontal">
    <dt>revision</dt>
    <dd>{{ repository.sdk-repository.tool.revision.major }}</dd>
    <dd>{{ repository.sdk-repository.tool.revision.minor }}</dd>
    <dt>min-platform-tools-rev</dt>
    <dd>{{ repository.sdk-repository.tool.min-platform-tools-rev.major }}</dd>
    {{#each repository.sdk-repository.tool.archives.archive}}
    <dt>size</dt>
    <dd>{{toAbbr size }}b</dd>
    <dt>checksum</dt>
    <dd>{{ checksum }}</dd>
    <dt>url</dt>
    <dd class="separator"><a class="btn btn-primary" href="http://dl-ssl.google.com/android/repository/{{ url }}">{{ url }}</a></dd>
    {{/each}}
  </dl>
  {{/if }}
