{{#prettify}}<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{package.description}}">
    <title>{{#is title "Viewer"}}{{ title }}{{else}}{{ title }} &middot; Viewer{{/is}}</title>
    <!-- Redirect from {{filename}} to {{basename}}/index.html -->
    <meta http-equiv="refresh" content="2;URL='{{basename}}/index.html'" />
  </head>
  <body>
    <!-- Note: don't tell people to `click` the link, just tell them that it is a link. -->
    If you are not redirected automatically, follow the <a href="{{basename}}/index.html">{{basename}}</a>
  </body>
</html>{{/prettify}}
